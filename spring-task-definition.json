{
    "family": "spring-task-definition",
    "containerDefinitions": [
        {
            "name": "ecommerce-example",
            "image": "390403887727.dkr.ecr.us-east-2.amazonaws.com/e-commerce-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "ecommerce-example-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/spring-task-definition",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::390403887727:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::390403887727:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "placementConstraints": [],
    "compatibilities": ["EC2", "FARGATE"],
    "volumes": [],
    "tags": []
}
